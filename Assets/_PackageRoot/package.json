{
  "name": "extensions.unity.saver",
  "displayName": "Unity Saver",
  "author": { "name": "Ivan Murzak", "url": "https://github.com/IvanMurzak" },
  "version": "1.0.9",
  "unity": "2019.2",
  "description": "Multiplatform encrypted database for Unity. Can be integrated in MonoBehaviour, ScriptableObject or be used such as simple C# class instance",
  "keywords": [
