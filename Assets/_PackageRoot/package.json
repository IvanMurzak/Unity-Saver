    "data",
    "unity"
  ],
  "dependencies": {
    "com.cysharp.unitask": "2.3.3",
    "com.neuecc.unirx": "7.1.0"
  },
  "scopedRegistries": [
    {
      "name": "package.openupm.com",
      "url": "https://package.openupm.com",
      "scopes": [
        "com.cysharp",
        "com.neuecc"
      ]
    }
  ]
}